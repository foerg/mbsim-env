<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<MBSimProject xmlns="http://www.mbsim-env.de/MBSimXML" name="Project">

  <Embed xmlns="http://www.mbsim-env.de/MBXMLUtils">
    <Parameter>
      <scalarParameter name="a" unit="-">1</scalarParameter>
      <scalarParameter name="b" unit="-">0.7</scalarParameter>
      <scalarParameter name="F0" unit="-">10</scalarParameter>
      <scalarParameter name="mu1" unit="-">0.3</scalarParameter>
      <scalarParameter name="mu2">0.5</scalarParameter>
      <scalarParameter name="mu0" unit="-">0.2</scalarParameter>
      <scalarParameter name="al" unit="-">atan(mu0+mu1)</scalarParameter>
    </Parameter>
    <DynamicSystemSolver xmlns="http://www.mbsim-env.de/MBSim" name="MBS">
      <plotFeatureRecursive value="generalizedRelativePosition">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedRelativeVelocity">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedForce">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedPosition">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedVelocity">true</plotFeatureRecursive>
      <frames/>
      <contours/>
      <groups/>
      <objects>
        <RigidBody name="Mass">
          <generalizedInitialPosition>
            <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <ele>0</ele>
              <ele>b/2*1.0001</ele>
            </xmlVector>
          </generalizedInitialPosition>
          <frameOfReference ref="../Object[Plane]/Frame[C]"/>
          <mass unit="kg">1</mass>
          <inertiaTensor unit="kg*m^2">
            <xmlMatrix xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <row>
                <ele>0.01</ele>
                <ele>0</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0.01</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0</ele>
                <ele>0.01</ele>
              </row>
            </xmlMatrix>
          </inertiaTensor>
          <stateDependentTranslation>
            <TranslationAlongAxesXY/>
          </stateDependentTranslation>
          <frames>
            <FixedRelativeFrame name="P1">
              <enableOpenMBV>
              </enableOpenMBV>
              <relativePosition unit="m">
                <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                  <ele>0</ele>
                  <ele>-b/2</ele>
                  <ele>0</ele>
                </xmlVector>
              </relativePosition>
            </FixedRelativeFrame>
            <FixedRelativeFrame name="P2">
              <enableOpenMBV>
              </enableOpenMBV>
              <relativePosition unit="m">
                <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                  <ele>-a/2</ele>
                  <ele>0</ele>
                  <ele>0</ele>
                </xmlVector>
              </relativePosition>
            </FixedRelativeFrame>
          </frames>
          <contours>
            <Point name="Point1">
              <frameOfReference ref="../Frame[P1]"/>
              <enableOpenMBV>
              </enableOpenMBV>
            </Point>
          </contours>
          <openMBVRigidBody>
            <Cuboid xmlns="http://www.mbsim-env.de/OpenMBV" name="Body1">
              <transparency unit="-">0.3</transparency>
              <initialTranslation unit="m">
                <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                  <ele>0</ele>
                  <ele>0</ele>
                  <ele>0</ele>
                </xmlVector>
              </initialTranslation>
              <initialRotation unit="rad">
                <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                  <ele>0</ele>
                  <ele>0</ele>
                  <ele>0</ele>
                </xmlVector>
              </initialRotation>
              <scaleFactor unit="-">1</scaleFactor>
              <length unit="m">
                <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                  <ele>a</ele>
                  <ele>b</ele>
                  <ele>1</ele>
                </xmlVector>
              </length>
            </Cuboid>
          </openMBVRigidBody>
          <enableOpenMBVFrameC>
          </enableOpenMBVFrameC>
        </RigidBody>
        <RigidBody name="Plane">
          <mass unit="kg">1</mass>
          <inertiaTensor unit="kg*m^2">
            <xmlMatrix xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <row>
                <ele>0.01</ele>
                <ele>0</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0.01</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0</ele>
                <ele>0.01</ele>
              </row>
            </xmlMatrix>
          </inertiaTensor>
          <timeDependentRotation>
            <CompositeFunction>
              <outerFunction>
                <RotationAboutZAxis/>
              </outerFunction>
              <innerFunction>
                <SymbolicFunction arg1="t">
                  1.01*al*(1-exp(-5*t))
                </SymbolicFunction>
              </innerFunction>
            </CompositeFunction>
          </timeDependentRotation>
          <frames>
            <FixedRelativeFrame name="P1">
              <enableOpenMBV>
              </enableOpenMBV>
              <relativeOrientation>
                <aboutZ xmlns="http://www.mbsim-env.de/MBXMLUtils" unit="degree">90</aboutZ>
              </relativeOrientation>
            </FixedRelativeFrame>
          </frames>
          <contours>
            <Line name="Line1">
              <frameOfReference ref="../Frame[P1]"/>
              <enableOpenMBV>
                <length unit="m">5</length>
              </enableOpenMBV>
            </Line>
          </contours>
          <enableOpenMBVFrameC>
          </enableOpenMBVFrameC>
        </RigidBody>
      </objects>
      <links>
        <Contact name="Contact">
          <connect ref1="../Object[Plane]/Contour[Line1]" ref2="../Object[Mass]/Contour[Point1]"/>
          <normalForceLaw>
            <UnilateralConstraint/>
          </normalForceLaw>
          <normalImpactLaw>
            <UnilateralNewtonImpact>
              <restitutionCoefficient unit="-">0</restitutionCoefficient>
            </UnilateralNewtonImpact>
          </normalImpactLaw>
          <tangentialForceLaw>
            <PlanarStribeckFriction>
              <frictionFunction>
                <SymbolicFunction arg1="gT">
                  mu0+mu1/(1+mu2*gT^2)
                </SymbolicFunction>
              </frictionFunction>
            </PlanarStribeckFriction>
          </tangentialForceLaw>
          <tangentialImpactLaw>
            <PlanarStribeckImpact>
              <frictionFunction>
                <SymbolicFunction arg1="gT">
                  mu0+mu1/(1+mu2*gT^2)
                </SymbolicFunction>
              </frictionFunction>
            </PlanarStribeckImpact>
          </tangentialImpactLaw>
        </Contact>
      </links>
      <constraints/>
      <observers>
        <FrameObserver name="FrameObserver">
          <frame ref="../Object[Mass]/Frame[C]"/>
          <enableOpenMBVVelocity>
            <referencePoint>"fromPoint"</referencePoint>
          </enableOpenMBVVelocity>
        </FrameObserver>
        <RigidBodyObserver name="RigidBodyObserver1">
          <rigidBody ref="../Object[Mass]"/>
          <enableOpenMBVWeight>
            <scaleLength unit="-">0.1</scaleLength>
          </enableOpenMBVWeight>
        </RigidBodyObserver>
        <ContactObserver name="ContactObserver1">
          <contact ref="../Link[Contact]"/>
          <enableOpenMBVNormalForce>
            <colorRepresentation>"absoluteValue"</colorRepresentation>
            <scaleLength unit="-">0.1</scaleLength>
          </enableOpenMBVNormalForce>
          <enableOpenMBVTangentialForce>
            <colorRepresentation>"stickslip"</colorRepresentation>
            <scaleLength unit="-">0.1</scaleLength>
          </enableOpenMBVTangentialForce>
        </ContactObserver>
      </observers>
      <enableOpenMBVFrameI>
      </enableOpenMBVFrameI>
      <environments>
        <MBSimEnvironment>
          <accelerationOfGravity unit="m/s^2">
            <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <ele>0</ele>
              <ele>-9.81</ele>
              <ele>0</ele>
            </xmlVector>
          </accelerationOfGravity>
        </MBSimEnvironment>
      </environments>
    </DynamicSystemSolver>
  </Embed>

  <LSODAIntegrator xmlns="http://www.mbsim-env.de/MBSim">
    <startTime unit="s">0</startTime>
    <endTime unit="s">9.5</endTime>
    <plotStepSize unit="s">1e-2</plotStepSize>
    <absoluteToleranceScalar>1e-6</absoluteToleranceScalar>
    <relativeToleranceScalar unit="-">1e-6</relativeToleranceScalar>
    <maximumStepSize unit="s">1e-2</maximumStepSize>
  </LSODAIntegrator>

</MBSimProject>
