<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<MBSimProject xmlns="http://www.mbsim-env.de/MBSimXML" name="Project">

  <Embed xmlns="http://www.mbsim-env.de/MBXMLUtils">
    <Parameter>
      <scalarParameter name="y0" unit="-">0.02</scalarParameter>
      <scalarParameter name="v0" unit="-">0.1</scalarParameter>
      <scalarParameter name="l" unit="-">0.1</scalarParameter>
      <scalarParameter name="r" unit="-">0.005</scalarParameter>
    </Parameter>
    <DynamicSystemSolver xmlns="http://www.mbsim-env.de/MBSim" name="MBS">
      <plotFeatureRecursive value="generalizedRelativePosition">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedRelativeVelocity">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedForce">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedPosition">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedVelocity">true</plotFeatureRecursive>
      <frames/>
      <contours>
        <PlanarContour name="PlanarContour1">
          <nodes unit="-">
            <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <ele>0</ele>
              <ele>2*l+2*pi*r</ele>
            </xmlVector>
          </nodes>
          <contourFunction>
            <ContinuedFunction>
              <function>
                <PiecewiseDefinedFunction>
                  <limitedFunctions>
                    <LimitedFunction>
                      <function>
                        <SymbolicFunction arg1="s">[l/2-s;r;0]

</SymbolicFunction>
                      </function>
                      <limit>l</limit>
                    </LimitedFunction>
                    <LimitedFunction>
                      <function>
                        <SymbolicFunction arg1="s">[-l/2-r*sin((s-l)/r);r*cos((s-l)/r);0]
</SymbolicFunction>
                      </function>
                      <limit>l+pi*r</limit>
                    </LimitedFunction>
                    <LimitedFunction>
                      <function>
                        <SymbolicFunction arg1="s">[s-(3/2*l+pi*r); -r; 0]
</SymbolicFunction>
                      </function>
                      <limit>2*l+pi*r</limit>
                    </LimitedFunction>
                    <LimitedFunction>
                      <function>
                        <SymbolicFunction arg1="s">[l/2+r*sin((s-2*l+pi*r)/r);-r*cos((s-2*l+pi*r)/r);0]
</SymbolicFunction>
                      </function>
                      <limit>2*l+2*pi*r</limit>
                    </LimitedFunction>
                  </limitedFunctions>
                </PiecewiseDefinedFunction>
              </function>
              <continuationRule>
                <ModuloFunction>
                  <denominator>2*l+2*pi*r</denominator>
                </ModuloFunction>
              </continuationRule>
            </ContinuedFunction>
          </contourFunction>
          <enableOpenMBV>
            <nodes unit="-">[0:0.0005:2*l+2*pi*r]';
</nodes>
          </enableOpenMBV>
        </PlanarContour>
      </contours>
      <groups/>
      <objects>
        <RigidBody name="RigidBody1">
          <generalizedInitialPosition>
            <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <ele>0</ele>
              <ele>y0</ele>
            </xmlVector>
          </generalizedInitialPosition>
          <generalizedInitialVelocity>
            <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <ele>v0</ele>
              <ele>0</ele>
            </xmlVector>
          </generalizedInitialVelocity>
          <mass unit="kg">1</mass>
          <inertiaTensor unit="kg*m^2">
            <xmlMatrix xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <row>
                <ele>0.01</ele>
                <ele>0</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0.01</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0</ele>
                <ele>0.01</ele>
              </row>
            </xmlMatrix>
          </inertiaTensor>
          <stateDependentTranslation>
            <TranslationAlongAxesXY/>
          </stateDependentTranslation>
          <frames/>
          <contours>
            <Point name="Point1">
              <enableOpenMBV>
              </enableOpenMBV>
            </Point>
          </contours>
          <enableOpenMBVFrameC>
            <size unit="cm">1</size>
          </enableOpenMBVFrameC>
        </RigidBody>
      </objects>
      <links>
        <Contact name="Contact1">
          <connect ref1="../Contour[PlanarContour1]" ref2="../Object[RigidBody1]/Contour[Point1]"/>
          <normalForceLaw>
            <UnilateralConstraint/>
          </normalForceLaw>
          <normalImpactLaw>
            <UnilateralNewtonImpact>
              <restitutionCoefficient unit="-">0</restitutionCoefficient>
            </UnilateralNewtonImpact>
          </normalImpactLaw>
          <searchAllContactPoints>1</searchAllContactPoints>
        </Contact>
      </links>
      <constraints/>
      <observers/>
      <enableOpenMBVFrameI>
        <size unit="cm">1</size>
      </enableOpenMBVFrameI>
      <environments>
        <MBSimEnvironment>
          <accelerationOfGravity unit="m/s^2">
            <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <ele>0</ele>
              <ele>-9.81</ele>
              <ele>0</ele>
            </xmlVector>
          </accelerationOfGravity>
        </MBSimEnvironment>
      </environments>
    </DynamicSystemSolver>
  </Embed>

  <LSODAIntegrator xmlns="http://www.mbsim-env.de/MBSim">
    <startTime unit="s">0</startTime>
    <endTime unit="s">1</endTime>
    <plotStepSize unit="s">1e-2</plotStepSize>
    <absoluteToleranceScalar>1e-6</absoluteToleranceScalar>
    <relativeToleranceScalar unit="-">1e-6</relativeToleranceScalar>
    <initialStepSize unit="s">1e-8</initialStepSize>
    <maximumStepSize unit="s">1e-2</maximumStepSize>
  </LSODAIntegrator>

</MBSimProject>
