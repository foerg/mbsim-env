<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<MBSimProject xmlns="http://www.mbsim-env.de/MBSimXML" name="Project">

  <Embed xmlns="http://www.mbsim-env.de/MBXMLUtils">
    <Parameter>
      <scalarParameter name="m" unit="-">0.7</scalarParameter>
      <scalarParameter name="a">0.6</scalarParameter>
      <scalarParameter name="b" unit="-">0.4</scalarParameter>
      <scalarParameter name="c">0.7</scalarParameter>
    </Parameter>
    <DynamicSystemSolver xmlns="http://www.mbsim-env.de/MBSim" name="MBS">
      <plotFeatureRecursive value="generalizedRelativePosition">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedRelativeVelocity">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedForce">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedPosition">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedVelocity">true</plotFeatureRecursive>
      <frames/>
      <contours/>
      <groups/>
      <objects>
        <RigidBody name="RigidBody1">
          <generalizedInitialPosition>
            <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <ele>0.01</ele>
              <ele>0.45</ele>
              <ele>0.01</ele>
            </xmlVector>
          </generalizedInitialPosition>
          <mass unit="kg">m</mass>
          <inertiaTensor unit="kg*m^2">
            <xmlMatrix xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <row>
                <ele>0.01</ele>
                <ele>0</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0.01</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0</ele>
                <ele>0.01</ele>
              </row>
            </xmlMatrix>
          </inertiaTensor>
          <stateDependentTranslation>
            <TranslationAlongAxesXYZ/>
          </stateDependentTranslation>
          <frames/>
          <contours>
            <Point name="Point1">
              <enableOpenMBV/>
            </Point>
          </contours>
          <enableOpenMBVFrameC>
            <size unit="cm">1</size>
          </enableOpenMBVFrameC>
        </RigidBody>
        <RigidBody name="RigidBody2">
          <mass unit="kg">1</mass>
          <inertiaTensor unit="kg*m^2">
            <xmlMatrix xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <row>
                <ele>0.01</ele>
                <ele>0</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0.01</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0</ele>
                <ele>0.01</ele>
              </row>
            </xmlMatrix>
          </inertiaTensor>
          <frames/>
          <contours>
            <SpatialContour name="SpatialContour3">
              <thickness unit="m">1</thickness>
              <etaNodes unit="-">
                <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                  <ele>0</ele>
                  <ele>2*pi</ele>
                </xmlVector>
              </etaNodes>
              <xiNodes unit="-">
                <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                  <ele>-pi/2</ele>
                  <ele>pi/2</ele>
                </xmlVector>
              </xiNodes>
              <contourFunction>
                <SymbolicFunction arg1="zeta" arg1Dim="2">
                  <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                    <ele>a*cos(zeta(1))*cos(zeta(2))</ele>
                    <ele>b*sin(zeta(1))*cos(zeta(2))</ele>
                    <ele>c*sin(zeta(2))</ele>
                  </xmlVector>
                </SymbolicFunction>
              </contourFunction>
              <enableOpenMBV>
              </enableOpenMBV>
            </SpatialContour>
          </contours>
          <enableOpenMBVFrameC>
            <size unit="cm">1</size>
          </enableOpenMBVFrameC>
        </RigidBody>
      </objects>
      <links>
        <Contact name="Contact1">
          <connect ref1="../Object[RigidBody2]/Contour[SpatialContour3]" ref2="../Object[RigidBody1]/Contour[Point1]"/>
          <normalForceLaw>
            <UnilateralConstraint/>
          </normalForceLaw>
          <normalImpactLaw>
            <UnilateralNewtonImpact>
              <restitutionCoefficient unit="-">0</restitutionCoefficient>
            </UnilateralNewtonImpact>
          </normalImpactLaw>
          <initialGuess>
            <xmlMatrix xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <row>
                <ele>pi/2</ele>
                <ele>0</ele>
              </row>
            </xmlMatrix>
          </initialGuess>
        </Contact>
      </links>
      <constraints/>
      <observers>
       <ContactObserver name="ContactObserver1">
          <contact ref="../Link[Contact1]"/>
          <enableOpenMBVContactPoints>
            <size unit="cm">1</size>
          </enableOpenMBVContactPoints>
        </ContactObserver>
      </observers>
      <enableOpenMBVFrameI>
        <size unit="cm">1</size>
      </enableOpenMBVFrameI>
      <environments>
        <MBSimEnvironment>
          <accelerationOfGravity unit="m/s^2">
            <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <ele>0</ele>
              <ele>-9.81</ele>
              <ele>0</ele>
            </xmlVector>
          </accelerationOfGravity>
        </MBSimEnvironment>
      </environments>
    </DynamicSystemSolver>
  </Embed>

  <LSODAIntegrator xmlns="http://www.mbsim-env.de/MBSim">
    <startTime unit="s">0</startTime>
    <endTime unit="s">1.5</endTime>
    <plotStepSize unit="s">1e-2</plotStepSize>
    <toleranceForPositionConstraints>1e-5</toleranceForPositionConstraints>
    <toleranceForVelocityConstraints>1e-5</toleranceForVelocityConstraints>
    <absoluteToleranceScalar>1e-7</absoluteToleranceScalar>
    <relativeToleranceScalar unit="-">1e-7</relativeToleranceScalar>
    <initialStepSize unit="s">1e-8</initialStepSize>
  </LSODAIntegrator>

</MBSimProject>
