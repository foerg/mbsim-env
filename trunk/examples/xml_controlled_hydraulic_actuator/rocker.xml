<?xml version="1.0" encoding="UTF-8"?>
<Group name="Rocker"
  xmlns="http://mbsim.berlios.de/MBSim"
  xmlns:c="http://mbsim.berlios.de/MBSimControl"
  xmlns:pv="http://openmbv.berlios.de/MBXMLUtils/physicalvariable"
  xmlns:ombv="http://openmbv.berlios.de/OpenMBV">
  <frameOfReference ref="../Frame[FRocker]"/>
  <frames/>
  <contours/>
  <groups/>
  <objects>
    <RigidBody name="Rocker">
      <frameOfReference ref="../Frame[I]"/>
      <frameForKinematics ref="Frame[Mount]"/>
      <mass>800*pi*.025^2*.5</mass>
      <inertiaTensor>800*.025^2*.5*(0.25^2/4+.5^2/12)*eye(3)</inertiaTensor>
      <translation/>
      <rotation>
        <RotationAboutFixedAxis>
          <axisOfRotation>
            [0; 0; 1]
          </axisOfRotation>
        </RotationAboutFixedAxis>
      </rotation>
      <frames>
        <frame>
          <Frame name="Mount"/>
          <position>[-.15; 0; 0]</position>
          <orientation>eye(3)</orientation>
        </frame>
        <frame>
          <Frame name="LeftPin"/>
          <frameOfReference ref="Frame[Mount]"/>
          <position>[-.1; 0; 0]</position>
          <orientation>eye(3)</orientation>
        </frame>
        <frame>
          <Frame name="RightPin"/>
          <frameOfReference ref="Frame[Mount]"/>
          <position>[.4; 0; 0]</position>
          <orientation>eye(3)</orientation>
        </frame>
      </frames>
      <contours/>
      <openMBVRigidBody>
        <ombv:Frustum name="x">
          <ombv:staticColor>.5</ombv:staticColor>
          <ombv:initialTranslation>[.25; 0; 0]</ombv:initialTranslation>
          <ombv:initialRotation>[0; pi/2; 0]</ombv:initialRotation>
          <ombv:scaleFactor>1</ombv:scaleFactor>
          <ombv:baseRadius>.025</ombv:baseRadius>
          <ombv:topRadius>.025</ombv:topRadius>
          <ombv:height>.5</ombv:height>
          <ombv:innerBaseRadius>0</ombv:innerBaseRadius>
          <ombv:innerTopRadius>0</ombv:innerTopRadius>
        </ombv:Frustum>
        <frameOfReference ref="Frame[C]"/>
      </openMBVRigidBody>
    </RigidBody>
    <RigidBody name="LeftPin">
      <frameOfReference ref="../Object[Rocker]/Frame[LeftPin]"/>
      <frameForKinematics ref="Frame[C]"/>
      <mass>800*.005^2*.06</mass>
      <inertiaTensor>800*pi*.005^2*.06*(0.005^2/4+.06^2/12)*eye(3)</inertiaTensor>
      <translation/>
      <rotation/>
      <frames>
      </frames>
      <contours>
      </contours>
      <openMBVRigidBody>
        <ombv:Frustum name="x">
          <ombv:staticColor>.2</ombv:staticColor>
          <ombv:initialTranslation>[0; 0; 0.03]</ombv:initialTranslation>
          <ombv:initialRotation>[0; 0; 0]</ombv:initialRotation>
          <ombv:scaleFactor>1</ombv:scaleFactor>
          <ombv:baseRadius>.005</ombv:baseRadius>
          <ombv:topRadius>.005</ombv:topRadius>
          <ombv:height>.06</ombv:height>
          <ombv:innerBaseRadius>0</ombv:innerBaseRadius>
          <ombv:innerTopRadius>0</ombv:innerTopRadius>
        </ombv:Frustum>
      </openMBVRigidBody>
    </RigidBody>
    <RigidBody name="RightPin">
      <frameOfReference ref="../Object[Rocker]/Frame[RightPin]"/>
      <frameForKinematics ref="Frame[C]"/>
      <mass>800*pi*.005^2*.06</mass>
      <inertiaTensor>800*.005^2*.06*(0.005^2/4+.06^2/12)*eye(3)</inertiaTensor>
      <translation>
        <LinearTranslation>
          <translationVectors>[1; 0; 0]</translationVectors>
        </LinearTranslation>
      </translation>
      <rotation/>
      <frames>
      </frames>
      <contours>
      </contours>
      <openMBVRigidBody>
        <ombv:Frustum name="x">
          <ombv:staticColor>.7</ombv:staticColor>
          <ombv:initialTranslation>[0; 0; 0.03]</ombv:initialTranslation>
          <ombv:initialRotation>[0; 0; 0]</ombv:initialRotation>
          <ombv:scaleFactor>1</ombv:scaleFactor>
          <ombv:baseRadius>.005</ombv:baseRadius>
          <ombv:topRadius>.005</ombv:topRadius>
          <ombv:height>.06</ombv:height>
          <ombv:innerBaseRadius>0</ombv:innerBaseRadius>
          <ombv:innerTopRadius>0</ombv:innerTopRadius>
        </ombv:Frustum>
      </openMBVRigidBody>
    </RigidBody>
    <RigidBody name="Mass">
      <frameOfReference ref="../../Frame[FMass]"/>
      <frameForKinematics ref="Frame[C]"/>
      <mass>800*pi*.1^2*.1</mass>
      <inertiaTensor>800*.1^2*.1*(.1^2/4+.1^2/12)*eye(3)</inertiaTensor>
      <translation>
        <LinearTranslation>
          <translationVectors>[0; 1; 0]</translationVectors>
        </LinearTranslation>
      </translation>
      <rotation/>
      <frames>
      </frames>
      <contours>
      </contours>
      <openMBVRigidBody>
        <ombv:Frustum name="x">
          <ombv:staticColor>1</ombv:staticColor>
          <ombv:initialTranslation>[0; 0; 0.03]</ombv:initialTranslation>
          <ombv:initialRotation>[pi/2; 0; 0]</ombv:initialRotation>
          <ombv:scaleFactor>1</ombv:scaleFactor>
          <ombv:baseRadius>.1</ombv:baseRadius>
          <ombv:topRadius>.1</ombv:topRadius>
          <ombv:height>.1</ombv:height>
          <ombv:innerBaseRadius>0</ombv:innerBaseRadius>
          <ombv:innerTopRadius>0</ombv:innerTopRadius>
        </ombv:Frustum>
      </openMBVRigidBody>
    </RigidBody>
  </objects>
  <links>
    <Joint name="MassJoint">
      <force>
        <direction>[1,0;0,1;0,0]</direction>
        <generalizedForceLaw>
          <BilateralConstraint/>
        </generalizedForceLaw>
        <generalizedImpactLaw>
          <BilateralImpact/>
        </generalizedImpactLaw>
      </force>
      <connect ref1="../Object[Mass]/Frame[C]" ref2="../Object[RightPin]/Frame[C]"/>
    </Joint>
  </links>
</Group>
