<?xml version="1.0" encoding="UTF-8"?>
<SpringDamper name="EXTERN" xmlns="http://mbsim.berlios.de/MBSim"
  xmlns:control="http://mbsim.berlios.de/MBSimControl">
  <forceFunction>
    <control:SignalFunction>
      <control:returnSignal ref="../Link[ExternForce]"/>
    </control:SignalFunction>
  </forceFunction>
  <connect ref1="../Frame[SpringMount]" ref2="../Object[Ball]/Frame[C]"/>
  <enableOpenMBVCoilSpring>
    <numberOfCoils>20</numberOfCoils>
    <springRadius>0.03</springRadius>
    <type>'polyline'</type>
  </enableOpenMBVCoilSpring>
</SpringDamper>
