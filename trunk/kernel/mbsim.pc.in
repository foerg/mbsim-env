prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@
bindir=${exec_prefix}/bin

Name: mbsim
Description: mbsim is a multi-body simulation library.
Requires: @requires@
Version: @VERSION@
Libs: -L${libdir} @CASADILDFLAGS@ -lmbsim @CASADILIB@ -lboost_thread -lboost_system
Cflags: -I${includedir} @DEFINES@ @CASADICPPFLAGS@ @openmbv_compilerflag@ -I/usr/include/boost

