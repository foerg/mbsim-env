  -->

  <xs:element name="Function3_MVVS" abstract="true" type="Function3_MVVSType"/>
  <xs:complexType name="Function3_MVVSType"/>
  
  <xs:element name="Function2_SVV" abstract="true" type="Function2_SVVType"/>
  <xs:complexType name="Function2_SVVType"/>

  <xs:element name="InfluenceFunction" abstract="true" type="InfluenceFunctionType">
    <xs:annotation>
      <xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
         Koppelt den Einfluss zwischen zwei Kontouren durch eine Einflussfunktion.
      </xs:documentation>
    </xs:annotation>
  </xs:element> 
  <xs:complexType name="InfluenceFunctionType">
    <xs:complexContent>
      <xs:extension base="Function2_SVVType">
        <xs:attribute name="contourName1" type="ContourReferenceType" use="required"/>
        <xs:attribute name="contourName2" type="ContourReferenceType" use="required"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  
  <xs:element name="FlexibilityInfluenceFunction" substitutionGroup="InfluenceFunction" type="FlexibilityInfluenceFunctionType">
    <xs:annotation>
      <xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
         Koppelt den Einfluss auf nur einer Kontur durch ihre Felxibilität.
      </xs:documentation>
    </xs:annotation>
  </xs:element>  
  <xs:complexType name="FlexibilityInfluenceFunctionType">
    <xs:complexContent>
      <xs:extension base="InfluenceFunctionType">
        <xs:sequence>
          <xs:element name="Flexibility" type="pv:unknownScalar"> <!-- Einheit eigentlich m/N -->
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
                Flexibilität (Kehrwert der Steifigkeit) der Kontur.
              </xs:documentation>
            </xs:annotation>
          </xs:element>            
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType> 

  <xs:element name="ConstantInfluenceFunction" substitutionGroup="InfluenceFunction" type="ConstantInfluenceFunctionType">
    <xs:annotation>
      <xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
         Koppelt den Einfluss zweier Konturen konsant.
      </xs:documentation>
    </xs:annotation>
  </xs:element> 
  <xs:complexType name="ConstantInfluenceFunctionType">
    <xs:complexContent>
      <xs:extension base="InfluenceFunctionType">
        <xs:sequence>
          <xs:element name="CouplingValue" type="pv:unknownScalar"> <!-- Einheit eigentlich m/N -->
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
                Kopplungswert zwischen zwei Konturen.
              </xs:documentation>
            </xs:annotation>
          </xs:element>            
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType> 

  <xs:element name="Observer" substitutionGroup="Element" type="ObserverType">
    <xs:annotation><xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
    tbd.
    </xs:documentation></xs:annotation>
  </xs:element>
  <xs:complexType name="ObserverType">
    <xs:complexContent>
      <xs:extension base="ElementType">
        <xs:attribute ref="xml:base"/>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:element name="AbsoluteKinematicsObserver" substitutionGroup="Observer" type="AbsoluteKinematicsObserverType">
    <xs:annotation><xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
    tbd.
    </xs:documentation></xs:annotation>
  </xs:element>
  <xs:complexType name="AbsoluteKinematicsObserverType">
    <xs:complexContent>
      <xs:extension base="ObserverType">
        <xs:sequence>
          <xs:element name="frame">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
              tbd.
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:attribute name="ref" type="FrameReferenceType" use="required"/>
            </xs:complexType>
          </xs:element>
          <xs:sequence>
            <xs:choice>
              <xs:element name="enableOpenMBVPosition" minOccurs="0">  
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="diameter" type="pv:lengthScalar">
                      <xs:annotation>
                        <xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
                        tbd.
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="headDiameter" type="pv:lengthScalar">
                      <xs:annotation>
                        <xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
                        tbd.
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="headLength" type="pv:lengthScalar">
                      <xs:annotation>
                        <xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
                        tbd.
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="staticColor" type="pv:nounitScalar">
                      <xs:annotation>
                        <xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
                        tbd.
                        </xs:documentation>
                      </xs:annotation>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType> 
              </xs:element>
              <xs:element name="openMBVPositionArrow" minOccurs="0">  
                 <xs:annotation>
                   <xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
                   tbd
                   </xs:documentation>
                 </xs:annotation>
                 <xs:complexType>
                   <xs:sequence>
                     <xs:element ref="ombv:Arrow"/>
                   </xs:sequence>
                 </xs:complexType>
              </xs:element>
            </xs:choice>
          </xs:sequence>
          <xs:element name="openMBVVelocityArrow" minOccurs="0">  
             <xs:annotation>
               <xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
               tbd
               </xs:documentation>
             </xs:annotation>
             <xs:complexType>
               <xs:sequence>
                 <xs:element ref="ombv:Arrow"/>
               </xs:sequence>
             </xs:complexType>
          </xs:element>
          <xs:element name="openMBVAngularVelocityArrow" minOccurs="0">  
             <xs:annotation>
               <xs:documentation xml:lang="de" xmlns="http://www.w3.org/1999/xhtml">
               tbd
               </xs:documentation>
