            </xs:documentation></xs:annotation>
            <xs:complexType>
              <xs:group ref="OpenMBVArrowParameters"/>
            </xs:complexType> 
          </xs:element>
        </xs:sequence> 
      </xs:extension>
    </xs:complexContent>
