<?xml version="1.0" encoding="UTF-8"?>
<DynamicSystemSolver name="MBS" xmlns="http://mbsim.berlios.de/MBSim"
    xmlns:pv="http://openmbv.berlios.de/MBXMLUtils/physicalvariable"
    xmlns:ombv="http://openmbv.berlios.de/OpenMBV">
  <relativePosition>[0;0;0]</relativePosition><!-- dummy -->
  <relativeOrientation>[1,0,0;0,1,0;0,0,1]</relativeOrientation><!-- dummy -->
  <frame>
    <Frame name="Os"/>
    <relativePosition>[0;0;0.04]</relativePosition>
    <relativeOrientation>[1,0,0;0,1,0;0,0,1]</relativeOrientation>
  </frame>
  <RigidBody name="Stab1">
    <initialGeneralizedPosition>[lO;0;pi/2]</initialGeneralizedPosition>
    <initialGeneralizedVelocity>[0;0;0]</initialGeneralizedVelocity>
    <frameOfReference ref="../Frame[I]"/>
    <frameForKinematics ref="Frame[C]"/>
    <mass>0.5</mass>
    <inertiaTensor>[0,0,0 ; 0,0,0 ; 0,0,0.041666666666666]</inertiaTensor>
    <translation>
      <LinearTranslation>
        <translationVectors>[1,0 ; 0,1 ; 0,0]</translationVectors>
      </LinearTranslation>
    </translation>
    <rotation>
      <RotationAboutFixedAxis>
        <axisOfRotation>[0;0;1]</axisOfRotation>
      </RotationAboutFixedAxis>
    </rotation>
    <frame>
      <Frame name="O"/>
      <relativePosition>[0;lO;0]</relativePosition>
      <relativeOrientation>[1,0,0;0,1,0;0,0,1]</relativeOrientation>
    </frame>
    <frame>
      <Frame name="U"/>
      <relativePosition>[0;-lU;0]</relativePosition>
      <relativeOrientation>[1,0,0;0,1,0;0,0,1]</relativeOrientation>
    </frame>
    <openMBVRigidBody>
      <ombv:Frustum name="dummy">
        <ombv:initialTranslation>[0;-0.5;0]</ombv:initialTranslation>
        <ombv:initialRotation>[1.5708;0;0]</ombv:initialRotation>
        <ombv:scaleFactor>1</ombv:scaleFactor>
        <ombv:baseRadius>0.02</ombv:baseRadius>
        <ombv:topRadius>0.02</ombv:topRadius>
        <ombv:height>1</ombv:height>
        <ombv:innerBaseRadius>0</ombv:innerBaseRadius>
        <ombv:innerTopRadius>0</ombv:innerTopRadius>
      </ombv:Frustum>
    </openMBVRigidBody>
  </RigidBody>
  <pv:embed href="Stab_n.xml" count="N" counterName="n"/>
  <Joint name="Gelenk1">
    <force>
      <direction>[1,0 ; 0,1 ; 0,0]</direction>
      <generalizedForceLaw>
        <BilateralConstraint/>
      </generalizedForceLaw>
      <generalizedImpactLaw>
        <BilateralImpact/>
      </generalizedImpactLaw>
    </force>
    <connect ref1="../Frame[I]" ref2="../Object[Stab1]/Frame[O]"/>
  </Joint>
  <pv:embed href="Joint_n.xml" count="N" counterName="n"/>
  <accelerationOfGravity>[0;-9.81;0]</accelerationOfGravity>
</DynamicSystemSolver>
