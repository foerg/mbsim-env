<?xml version="1.0" encoding="UTF-8"?>
<DynamicSystemSolver name="TS" xmlns="http://mbsim.berlios.de/MBSim"
    xmlns:ombv="http://openmbv.berlios.de/OpenMBV"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://mbsim.berlios.de/MBSim /usr/users/fermat/friedma/project/MBSimNeu/local/share/mbsim/schema/mbsim.xsd">
  <relativeGroupLocation>[0;0;0]</relativeGroupLocation><!-- dummy -->
  <relativeGroupOrientation>[1,0,0;0,1,0;0,0,1]</relativeGroupOrientation><!-- dummy -->
  <Plane name="Plane"/>
  <relativeContourLocation>[0;0;0]</relativeContourLocation>
  <relativeContourOrientation>[0,-1,0;1,0,0;0,0,1]</relativeContourOrientation>
  <RigidBody name="Body">
    <q0>[0;1.2015e-2;0;0.1;0;0]</q0>
    <u0>[0;0;0;0;180;0]</u0>
    <frameOfReference ref="../Frame[I]"/>
    <frameForKinematics ref="Frame[C]"/>
    <mass>6e-3</mass>
    <inertiaTensor>[8e-7,0,0 ; 0,7e-7,0 ; 0,0,8e-7]</inertiaTensor>
    <LinearTranslation>
      <PJT>[1,0,0;0,1,0;0,0,1]</PJT>
    </LinearTranslation>
    <CardanAngles/>
    <Sphere name="Sphere1">
      <radius>1.5e-2</radius>
      <enableOpenMBV/>
    </Sphere>
    <relativeContourLocation>[0;0.3e-2;0]</relativeContourLocation>
    <relativeContourOrientation>[1,0,0;0,1,0;0,0,1]</relativeContourOrientation>
    <Sphere name="Sphere2">
      <radius>0.5e-2</radius>
      <enableOpenMBV/>
    </Sphere>
    <relativeContourLocation>[0;1.6e-2;0]</relativeContourLocation>
    <relativeContourOrientation>[1,0,0;0,1,0;0,0,1]</relativeContourOrientation>
    <enableOpenMBVFrameC>
      <size>0.036</size>
      <offset>0</offset>
    </enableOpenMBVFrameC>
  </RigidBody>
  <Contact name="Contact1">
    <UnilateralConstraint/>
    <UnilateralNewtonImpact>
      <restitutionCoefficient>0.0</restitutionCoefficient>
    </UnilateralNewtonImpact>
    <SpatialCoulombFriction>
      <frictionCoefficient>0.2</frictionCoefficient>
    </SpatialCoulombFriction>
    <SpatialCoulombImpact>
      <frictionCoefficient>0.2</frictionCoefficient>
    </SpatialCoulombImpact>
    <connect ref1="../Contour[Plane]" ref2="../Object[Body]/Contour[Sphere1]"/>
  </Contact>
  <Contact name="Contact2">
    <UnilateralConstraint/>
    <UnilateralNewtonImpact>
      <restitutionCoefficient>0.0</restitutionCoefficient>
    </UnilateralNewtonImpact>
    <SpatialCoulombFriction>
      <frictionCoefficient>0.2</frictionCoefficient>
    </SpatialCoulombFriction>
    <SpatialCoulombImpact>
      <frictionCoefficient>0.2</frictionCoefficient>
    </SpatialCoulombImpact>
    <connect ref1="../Contour[Plane]" ref2="../Object[Body]/Contour[Sphere2]"/>
  </Contact>
  <accelerationOfGravity>[0;-9.81;0]</accelerationOfGravity>
</DynamicSystemSolver>
