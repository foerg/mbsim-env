<?xml version="1.0" encoding="UTF-8"?>
<Group name="HydraulicActor"
  xmlns="http://mbsim.berlios.de/MBSim"
  xmlns:c="http://mbsim.berlios.de/MBSimControl"
  xmlns:h="http://mbsim.berlios.de/MBSimHydraulics"
  xmlns:pv="http://openmbv.berlios.de/MBXMLUtils/physicalvariable"
  xmlns:ombv="http://openmbv.berlios.de/OpenMBV">
  <frameOfReference ref="../Frame[FJack]"/>
  <frames>
  </frames>
  <contours/>
  <groups/>
  <objects>
    <RigidBody name="HydraulicActor">
      <frameOfReference ref="../../Frame[FJack]"/>
      <frameForKinematics ref="Frame[FMount]"/>
      <mass>1.</mass>
      <inertiaTensor>1e-4*eye(3)</inertiaTensor>
      <translation/>
      <rotation>
        <RotationAboutFixedAxis>
          <axisOfRotation>[0; 0; 1]</axisOfRotation>
        </RotationAboutFixedAxis>
      </rotation>
      <frames>
        <frame>
          <Frame name="FMount">
            <enableOpenMBV>
              <size>.02</size>
              <offset>1</offset>
            </enableOpenMBV>
          </Frame>
          <frameOfReference ref="Frame[C]"/>
          <position>[0; -.11; 0]</position>
          <orientation>eye(3)</orientation>
        </frame>
        <frame>
          <Frame name="FBottom"/>
          <frameOfReference ref="Frame[FMount]"/>
          <position>[0; .01; 0]</position>
          <orientation>eye(3)</orientation>
        </frame>
        <frame>
          <Frame name="FPlunger"/>
          <frameOfReference ref="Frame[FBottom]"/>
          <position>[0; .05+.01; 0]</position>
          <orientation>eye(3)</orientation>
        </frame>
        <frame>
          <Frame name="FTop"/>
          <frameOfReference ref="Frame[FPlunger]"/>
          <position>[0; .01+.05; 0]</position>
          <orientation>eye(3)</orientation>
        </frame>
      </frames>
      <contours/>
      <openMBVRigidBody>
        <ombv:CompoundRigidBody name="x">
          <ombv:staticColor>.4</ombv:staticColor>
          <ombv:initialTranslation>[0; 0; 0]</ombv:initialTranslation>
          <ombv:initialRotation>[0; 0; 0]</ombv:initialRotation>
          <ombv:scaleFactor>1</ombv:scaleFactor>
          <ombv:Frustum name="x">
            <ombv:staticColor>.4</ombv:staticColor>
            <ombv:initialTranslation>[0; 0; 0]</ombv:initialTranslation>
            <ombv:initialRotation>[pi/2; 0; 0]</ombv:initialRotation>
            <ombv:scaleFactor>1</ombv:scaleFactor>
            <ombv:baseRadius>.03</ombv:baseRadius>
            <ombv:topRadius>.03</ombv:topRadius>
            <ombv:height>.01</ombv:height>
            <ombv:innerBaseRadius>0</ombv:innerBaseRadius>
            <ombv:innerTopRadius>0</ombv:innerTopRadius>
          </ombv:Frustum>
          <ombv:Frustum name="x">
            <ombv:staticColor>.4</ombv:staticColor>
            <ombv:initialTranslation>[0; .13; 0]</ombv:initialTranslation>
            <ombv:initialRotation>[pi/2; 0; 0]</ombv:initialRotation>
            <ombv:scaleFactor>1</ombv:scaleFactor>
            <ombv:baseRadius>.03</ombv:baseRadius>
            <ombv:topRadius>.03</ombv:topRadius>
            <ombv:height>.01</ombv:height>
            <ombv:innerBaseRadius>.01</ombv:innerBaseRadius>
            <ombv:innerTopRadius>.01</ombv:innerTopRadius>
          </ombv:Frustum>
        </ombv:CompoundRigidBody>
        <frameOfReference ref="Frame[FMount]"/>
      </openMBVRigidBody>
    </RigidBody>
    <RigidBody name="Plunger">
      <frameOfReference ref="../Object[HydraulicActor]/Frame[FPlunger]"/>
      <frameForKinematics ref="Frame[C]"/>
      <mass>1.</mass>
      <inertiaTensor>1e-4*eye(3)</inertiaTensor>
      <translation>
        <LinearTranslation>
          <translationVectors>[0;1;0]</translationVectors>
        </LinearTranslation>
      </translation>
      <rotation/>
      <frames>
        <frame>
          <Frame name="FTop"/>
          <position>[0; .01; 0]</position>
          <orientation>eye(3)</orientation>
        </frame>
        <frame>
          <Frame name="FBottom"/>
          <position>[0; -.01; 0]</position>
          <orientation>eye(3)</orientation>
        </frame>
        <frame>
          <Frame name="Top">
            <enableOpenMBV>
              <size>.05</size>
              <offset>1</offset>
            </enableOpenMBV>
          </Frame>
          <position>[0; .13; 0]</position>
          <orientation>eye(3)</orientation>
        </frame>
      </frames>
      <contours/>
      <openMBVRigidBody>
        <ombv:CompoundRigidBody name="x">
          <ombv:staticColor>.1</ombv:staticColor>
          <ombv:initialTranslation>[0; 0; 0]</ombv:initialTranslation>
          <ombv:initialRotation>[0; 0; 0]</ombv:initialRotation>
          <ombv:scaleFactor>1</ombv:scaleFactor>
          <ombv:Frustum name="x">
            <ombv:staticColor>.1</ombv:staticColor>
            <ombv:initialTranslation>[0; -.01; 0]</ombv:initialTranslation>
            <ombv:initialRotation>[pi/2; 0; 0]</ombv:initialRotation>
            <ombv:scaleFactor>1</ombv:scaleFactor>
            <ombv:baseRadius>.03</ombv:baseRadius>
            <ombv:topRadius>.03</ombv:topRadius>
            <ombv:height>.02</ombv:height>
            <ombv:innerBaseRadius>0</ombv:innerBaseRadius>
            <ombv:innerTopRadius>0</ombv:innerTopRadius>
          </ombv:Frustum>
          <ombv:Frustum name="x">
            <ombv:staticColor>.1</ombv:staticColor>
            <ombv:initialTranslation>[0; .01; 0]</ombv:initialTranslation>
            <ombv:initialRotation>[pi/2; 0; 0]</ombv:initialRotation>
            <ombv:scaleFactor>1</ombv:scaleFactor>
            <ombv:baseRadius>.01</ombv:baseRadius>
            <ombv:topRadius>.01</ombv:topRadius>
            <ombv:height>.12</ombv:height>
            <ombv:innerBaseRadius>0</ombv:innerBaseRadius>
            <ombv:innerTopRadius>0</ombv:innerTopRadius>
          </ombv:Frustum>
        </ombv:CompoundRigidBody>
      </openMBVRigidBody>
    </RigidBody>
    <!--
    <h:CircularLeakageLine name="Leckage">
      <frameOfReference ref="../Object[HydraulicActor]/Frame[FPlunger]"/>
      <h:direction>[0;0;0]</h:direction>
      <h:length>.5</h:length>
      <h:innerRadius>.03</h:innerRadius>
      <h:height>50e-6</h:height>
      <h:circularLeakagePressureLoss>
        <h:RealCircularLeakagePressureLoss/>
      </h:circularLeakagePressureLoss>
    </h:CircularLeakageLine>
    -->
  </objects>
  <links>
    <h:RigidNodeMec name="nA">
      <!--
      <h:outflow ref="../HLine[Leckage]"/>
      -->
      <h:inflow ref="../../Group[HydraulicSystem]/HLine[lineA]"/>
      <h:enableOpenMBVSphere>
        <h:size>.02</h:size>
        <h:minimalPressure unit="bar">10</h:minimalPressure>
        <h:maximalPressure unit="bar">50</h:maximalPressure>
      </h:enableOpenMBVSphere>
      <h:initialVolume>pi*(.03^2-.01^2)*.05</h:initialVolume>
      <h:translatorialBoundarySourface>
        <h:frameOfReference ref="../Object[HydraulicActor]/Frame[FTop]"/>
        <h:normal>[0; 1; 0]</h:normal>
        <h:area>pi*(.03^2-.01^2)</h:area>
      </h:translatorialBoundarySourface>
      <h:translatorialBoundarySourface>
        <h:frameOfReference ref="../Object[Plunger]/Frame[FTop]"/>
        <h:normal>[0; -1; 0]</h:normal>
        <h:area>pi*(.03^2-.01^2)</h:area>
      </h:translatorialBoundarySourface>
      <h:enableOpenMBVArrows> <!-- optional -->
        <h:size>.01</h:size>
      </h:enableOpenMBVArrows>
      <!--
      <h:initialPressure unit="bar">2</h:initialPressure>
      <h:fracAir>.05</h:fracAir>
      -->
    </h:RigidNodeMec>
    <h:RigidNodeMec name="nB">
      <!--
      <h:inflow ref="../HLine[Leckage]"/>
      -->
      <h:inflow ref="../../Group[HydraulicSystem]/HLine[lineB]"/>
      <h:enableOpenMBVSphere>
        <h:size>.02</h:size>
        <h:minimalPressure unit="bar">10</h:minimalPressure>
        <h:maximalPressure unit="bar">50</h:maximalPressure>
      </h:enableOpenMBVSphere>
      <h:initialVolume>pi*.03^2*.05</h:initialVolume>
      <h:translatorialBoundarySourface>
        <h:frameOfReference ref="../Object[HydraulicActor]/Frame[FBottom]"/>
        <h:normal>[0;-1 ; 0]</h:normal>
        <h:area>pi*.03^2</h:area>
      </h:translatorialBoundarySourface>
      <h:translatorialBoundarySourface>
        <h:frameOfReference ref="../Object[Plunger]/Frame[FBottom]"/>
        <h:normal>[0; 1; 0]</h:normal>
        <h:area>pi*.03^2</h:area>
      </h:translatorialBoundarySourface>
      <h:enableOpenMBVArrows>
        <h:size>.01</h:size>
      </h:enableOpenMBVArrows>
      <!--
      <h:initialPressure unit="bar">2</h:initialPressure>
      <h:fracAir>.05</h:fracAir>
      -->
    </h:RigidNodeMec>
    <h:EnvironmentNodeMec name="nPlungerEnvironment">
      <h:initialVolume>0</h:initialVolume>
      <h:translatorialBoundarySourface>
        <h:frameOfReference ref="../Object[Plunger]/Frame[FTop]"/>
        <h:normal>[0; -1; 0]</h:normal>
        <h:area>pi*.01^2</h:area>
      </h:translatorialBoundarySourface>
      <h:enableOpenMBVArrows>
        <h:size>.01</h:size>
      </h:enableOpenMBVArrows>
    </h:EnvironmentNodeMec>
  </links>
</Group>
