<Group name="env" xmlns="http://openmbv.berlios.de/OpenMBV">
  <Rotation name="rotation">
    <initialTranslation>[0;-3;0]</initialTranslation>
    <initialRotation>[0;0;0]</initialRotation>
    <scaleFactor>1</scaleFactor>
    <startAngle>0.5</startAngle>
    <endAngle>1</endAngle>
    <contour>
      [ 0, 0, 1;
        1, 0, 1;
        1, 1, 1 ]
    </contour>
  </Rotation>
  <Extrusion name="extrusion">
    <initialTranslation>[2;-3;0]</initialTranslation>
    <initialRotation>[0;0;0]</initialRotation>
    <scaleFactor>1</scaleFactor>
    <windingRule>"odd"</windingRule>
    <height>0.5</height>
    <contour>
      [ 0, 0, 1;
        1, 0, 1;
        1, 1, 1 ]
    </contour>
    <contour>
      [ 0.6, 0.2, 1;
        0.8, 0.2, 1;
        0.8, 0.4, 1;
        0.6, 0.4, 1 ]
    </contour>
  </Extrusion>
</Group>
