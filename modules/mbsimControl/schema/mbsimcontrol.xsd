  <xs:element name="Multiplexer" substitutionGroup="Signal" type="MultiplexerType">
    <xs:annotation>
      <xs:documentation xml:lang="de" xmlns="">
        Vektorwertiges Zusammenführen von Signalen
      </xs:documentation>
