<?xml version="1.0"?>
<mime-info xmlns='http://www.freedesktop.org/standards/shared-mime-info'>
  <mime-type type="application/vnd.mbsim-env.mbsimflatxml">
    <comment>MBSim-Env MBSimXML flat files</comment>
    <glob pattern="*.mbsimprj.flat.xml"/>
    <sub-class-of type="application/xml"/>
    <icon name="mbsim-env.mbsimgui"/>
  </mime-type>
  <mime-type type="application/vnd.mbsim-env.mbsimxml">
    <comment>MBSim-Env MBSimXML preprocessed files</comment>
    <glob pattern="*.mbsimprj.xml"/>
    <sub-class-of type="application/xml"/>
    <icon name="mbsim-env.mbsimgui"/>
  </mime-type>
</mime-info>
